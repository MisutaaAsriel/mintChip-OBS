OBSThemeMeta {
    dark: 'true';
    author: 'Asriel Dreemurr (Azi) â™¡';
}

OBSTheme {
	window: palette(base);
	base: rgb(32, 39, 47);
	alternateBase: palette(dark);
	text: rgb(238, 238, 238);
	
	light: rgb(87, 107, 126);
	mid: rgb(47, 57, 66);
	dark: rgb(14, 31, 39);
	shadow: rgb(9, 22, 30);
	
	button: palette(mid);
	highlight: rgb(33, 202, 161);
}


OBSTheme::disabled {
	text: #eeeeee40;
}

