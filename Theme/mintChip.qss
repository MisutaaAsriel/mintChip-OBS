			width: 20px;
			height: 20px;
			margin: -10px 0px 0px 0px;
		}

		SourceTreeSubItemCheckBox::indicator:hover {
			background-color: palette(light);
		}

		SourceTreeSubItemCheckBox::indicator:pressed {
			background-color: palette(highlight);
