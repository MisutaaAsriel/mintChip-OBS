OBSTheme {
	window: rgb(32, 39, 47);
	base: rgb(32, 39, 47);
	alternateBase: rgb(14, 31, 39);
	windowText: rgb(238, 238, 238);
	
	light: rgb(87, 107, 126);
	mid: rgb(47, 57, 66);
	dark: rgb(14, 31, 39);
	shadow: rgb(9, 22, 30);
	
	button: rgb(47, 57, 66);
}


OBSTheme::disabled {
	text: #eeeeee40;
}

/* #region Universal Elements */

	/* #region Primary Elements */
	* {
		color: rgb(238, 238, 238);
		font-family: "SF Compact Rounded", "Arial Rounded MT", Noto Sans, "Segoe UI", Helvetica, Tahoma, Arial;

		font-weight: bold;
		font-size: 14px;
		
		margin: 0px;
		padding: 0px;
		border: none;
		border-radius: 0px;
		background-color: transparent;
		position: relative;
	}

	*:disabled {
		color: rgb(87, 107, 126); /* Disabled Text Color */
	}

	QMainWindow,
	QDialog,
	QWidget,
	QFrame {
		background-color: rgb(32, 39, 47); /* Window Background */
		selection-background-color: rgb(33, 202, 161); 
		selection-color: rgb(238, 238, 238); 
		outline: none;

		qproperty-windowIcon: url(./Mintchip/icon_256x256.png); /* Window Icon */
	}

	QMainWindow {
		padding: 16px;
	}

	QWidget::item:hover {
		background-color: rgb(47, 57, 66); /* Hover Background */
	}

	QSizeGrip {
		image: url(./Mintchip/Controls/arrow-down-right.svg);
		min-width: 8px;
		min-height: 8px;
		margin: 0px;
	} /* Widget Size Grips */

		/* #region Dock Separators */
			QMainWindow::separator {
				width: 8px;
				height: 8px;
				margin: 6px;
				padding: -2px;
				border-radius: 2px;
				background-color: rgba(87, 107, 126, 0.1);
			}

			QMainWindow::separator:hover {
				background-color: rgb(87, 107, 126);
			}
		/* #endregion */

	/* #endregion */

	/* #region Buttons */
		QPushButton,
		QToolButton {
			border-radius: 6px;
			color: rgb(238, 238, 238);
		}

		QPushButton {
			padding: 4px 12px;
			min-width: 36px;
			background-color: rgb(47, 57, 66);
		}
		
		QPushButton:hover,
		QToolButton:hover,
		QTabBar::tab:!selected:hover {
			background: rgb(87, 107, 126);
		}

		QPushButton:pressed,
		QToolButton:pressed,
		QPushButton:checked:pressed {
			background: rgb(33, 202, 161);
		}
	/* #endregion */

	/* #region Docks */
		QDockWidget > QWidget > QWidget {
			padding: 8px 16px;
		}

		QDockWidget {
			background: rgb(32, 39, 47);
			min-width: 320px;
		}

		QDockWidget::title {
			background: rgb(32, 39, 47);
			padding: 20px 0px 0px 0px;
			margin: 0px;
			margin-left: 18px;
			subcontrol-position: left center;
		}

		/* QDockWidget::title:closable {
			margin-left: 8px;
		} Fixes Alignment Issue When Closable

		QDockWidget::title:!movable {
			margin-left: 18px;
		} Fixes Alignment Issue When Locked */

		QDockWidget {
			titlebar-close-icon: url('./Mintchip/close.svg');
			titlebar-normal-icon: url('./Mintchip/window.svg');
		}

		QDockWidget::close-button, QDockWidget::float-button {
			border-radius: 6px;
			background-color: transparent;

			icon-size: 36px;

			min-width: 16px;
			min-height: 16px;
			max-width: 16px;
			max-height: 16px;

			top: -4px;
		}

		QDockWidget::float-button {
			subcontrol-position: right center;
			right: 24px;
			padding: 5px;
		}

		QDockWidget::close-button {
			subcontrol-position: right center;
			right: 24px;
			padding: 4px;
		}

		QDockWidget::close-button:movable {
			right: 56px;
		}

		QDockWidget::close-button:hover, QDockWidget::float-button:hover {
			background-color: rgb(47, 57, 66);
		}

		QDockWidget::close-button:pressed, QDockWidget::float-button:pressed {
			padding: 1px -1px -1px 1px;
			background-color: rgb(47, 57, 66);
		}
	/* #endregion */

	/* #region Lists */
		QListWidget, QListWidget > *, QListWidget::item {
			background-color: rgb(9, 22, 30);
			border-radius: 6px;
			padding: 8px;
		}

		/* QListWidget * {
			background-color: rgb(9, 22, 30);
		} */

		QListWidget::item {
			margin-bottom: 4px;
			min-height: 26px;
			margin-right: 8px;
		}



		QListWidget::item:selected {
			background-color: rgb(14, 31, 39);
		}

		QListWidget::item:hover,
		QListWidget::item:disabled:hover,
		QListWidget::item:hover:!active {
			background-color: rgb(47, 57, 66);
		}

		.QVBoxLayout {
			background: rgb(9, 22, 30);
			border-radius: 6px;
		}
	/*#endregion*/

	/* #region Menus */
		QMenuBar::item {
			background: rgb(32, 39, 47);
			padding: 4px 8px;
			margin-right: 4px;
		}

		QMenuBar::item:selected {
			background-color: rgb(33, 202, 161);
			color: rgb(9, 22, 30);
			border-radius: 4px;
		}

		QMenuBar::item:pressed {
			border: none;
			background-color: rgb(163, 255, 231);
			color: rgb(9, 22, 30);
		}

		QMenuBar {
			padding: 4px;
		}

		QMenu {
			padding: 8px;
			background: rgba(32, 39, 47, 0.99);
			border-radius: 6px;
		}

		QMenu::item {
			padding: 6px 8px;
			padding-right: 16px;
		}

		QMenu::indicator {
			padding-left: 8px;
			width: 14px;
			height: 14px;
		}

		QMenu::separator {
			background-color: rgba(47, 57, 66, 0.75);
			border-radius: 1px;
			height: 2px;
			margin: 6px;
		}

		QMenu::item:selected {
			background-color: rgb(47, 57, 66);
			border-radius: 4px;
		}

		QMenu::indicator:non-exclusive:unchecked {
		image: url(./Mintchip/Controls/square.svg);
		}

		QMenu::indicator:non-exclusive:unchecked:selected {
		image: url(./Mintchip/Controls/square-hover.svg);
		}

		QMenu::indicator:non-exclusive:checked {
		image: url(./Mintchip/Controls/square-full.svg);
		}

		QMenu::indicator:non-exclusive:checked:selected {
		image: url(./Mintchip/Controls/square-full-hover.svg);
		}
	/* #endregion */

	/* #region Scrollbars */
		QScrollBar {
			border-radius: 8px;
			background-color: rgb(6, 15, 19);
		}

		

		QScrollBar::up-arrow,
		QScrollBar::down-arrow,
		QScrollBar::add-page {
			background-color: transparent;
		}

		QScrollBar::handle {
			background-color: rgb(47, 57, 66);
			min-width: 3px;
			min-height: 3px;
			margin: 3px;
			border-radius: 5px;
			width: 10px;
			height: 10px;
			padding: 0px;
		}

		QScrollBar::handle:hover {
			background-color: rgb(87, 107, 126);
		}

		QAbstractScrollArea::corner {
			width: 12px;
			height: 12px;
			margin: 2px;
			border: none;
			border-radius: 6px;
			background-color: rgb(6, 15, 19);
		}

		QScrollBar:horizontal {
			height: 16px;
		}

		QScrollBar:vertical {
			width: 16px;
		}

		QScrollBar::add-line:horizontal,
		QScrollBar::sub-line:horizontal,
		QScrollBar::add-line:horizontal:hover,
		QScrollBar::add-line:horizontal:on,
		QScrollBar::sub-line:horizontal:hover,
		QScrollBar::sub-line:horizontal:on,
		QScrollBar::add-line:vertical,
		QScrollBar::sub-line:vertical,
		QScrollBar::add-line:vertical:hover,
		QScrollBar::add-line:vertical:on,
		QScrollBar::sub-line:vertical:hover,
		QScrollBar::sub-line:vertical:on {
			margin: 0px;
			border-image: none;
			height: 0px;
			width: 0px;
		}

		QScrollBar::add-line:horizontal,
		QScrollBar::add-line:horizontal:hover,
		QScrollBar::add-line:horizontal:on {
			subcontrol-position: right;
			subcontrol-origin: margin;
		}

		QScrollBar::sub-line:horizontal,
		QScrollBar::sub-line:horizontal:hover,
		QScrollBar::sub-line:horizontal:on {
			subcontrol-position: left;
			subcontrol-origin: margin;
		}

		QScrollBar::sub-line:vertical,
		QScrollBar::sub-line:vertical:hover,
		QScrollBar::sub-line:vertical:on {
			subcontrol-position: top;
			subcontrol-origin: margin;
		}

		QScrollBar::add-line:vertical,
		QScrollBar::add-line:vertical:hover,
		QScrollBar::add-line:vertical:on {
			subcontrol-position: bottom;
			subcontrol-origin: margin;
		}
	/* #endregion */

	/* #region Tabs */
		QTabWidget::pane {
			border: none;
			border-radius: 0px;
			padding: 8px;
			background-color: transparent;
		}

		QTabWidget::tab-bar {
			top: 8px;
			border: none;
			background-color: transparent;
		}

		QTabBar {
			border-radius: 0px;
		}

		QTabBar::close-button {
			image: url(./Mintchip/close.svg);
			background: transparent;
		}

		QTabBar::close-button:hover {
			background: rgb(47, 57, 66);
		}

		QTabBar::close-button:pressed {
			image: url(./Mintchip/close.svg);
			background: rgb(87, 107, 126);
		}

		QTabBar::tab {
			background-color: rgb(33, 202, 161);
			color: rgb(9, 22, 30); 
			border-radius: 4px;
			margin: 2px;
			padding: 8px;
			min-width: 50px;
		}

		QTabBar::tab:!selected {
			background-color: rgb(47, 57, 66);
			color: rgb(238, 238, 238);
		}
	/* #endregion */

	/* #region Toolbars */
		QToolBar {
			background-color: rgb(9, 22, 30);
			border-radius: 6px;
			margin: 8px 0px 0px 0px;
			min-height: 48px;
			max-height: 48px;
		}

		QToolBar > QToolButton {
			subcontrol-position: left top;
			padding: 4px;
			margin: 8px 4px 0px 0px;
			min-height: 16px;
			min-width: 16px;
		}

		QToolBar::separator {
			border: none;
			background-color: rgba(47, 57, 66, 0.5);
			border-radius: 1px;
			height: 2px;
			width: 2px;
			margin: 8px 4px 0px 0px;
		}
	/* #endregion */

	/* #region Spinboxes */
		QAbstractSpinBox {
			padding: 8px;
			border: 0px solid rgb(87, 107, 126);/* Light Gray */
			background-color: rgb(9, 22, 30);
			color: rgb(238, 238, 238); /* White */
			border-radius: 6px;
			min-width: 75px;
		}

		QAbstractSpinBox:up-button {
			background-color: transparent;
			subcontrol-origin: border;
			subcontrol-position: top right;
			margin-right: 4px;
		}

		QAbstractSpinBox:down-button {
			background-color: transparent;
			subcontrol-origin: border;
			subcontrol-position: bottom right;
			margin-right: 4px;
		}

		QAbstractSpinBox::up-arrow,
		QAbstractSpinBox::up-arrow:disabled,
		QAbstractSpinBox::up-arrow:off {
			image: url(./Mintchip/Controls/angle-up-disabled.svg);
			width: 16px;
			height: 16px;
		}

		QAbstractSpinBox::up-arrow:hover {
			image: url(./Mintchip/Controls/angle-up.svg);
		}

		QAbstractSpinBox::down-arrow,
		QAbstractSpinBox::down-arrow:disabled,
		QAbstractSpinBox::down-arrow:off {
			image: url(./Mintchip/Controls/angle-down-disabled.svg);
			width: 16px;
			height: 16px;
		}

		QAbstractSpinBox::down-arrow:hover {
			image: url(./Mintchip/Controls/angle-down.svg);
		}
	/* #endregion */

	/* #region Textboxes */
		QLineEdit {
			background-color: rgb(9, 22, 30);
			padding: 8px;
			border: 0px solid rgb(87, 107, 126);
			border-radius: 6px;
			min-height: 16px;
		}

		QListWidget QLineEdit {
			padding-top: 0;
			padding-bottom: 0;
			padding-right: 0;
			padding-left: 2px;
			border: none;
			border-radius: none;
		}
	/* #endregion */

	/* #region Checkboxes */
		QCheckBox {
			spacing: 5px;
			outline: none;
			color: rgb(238, 238, 238);
			margin-bottom: 2px;
			padding: 4px;
			border-radius: 6px;
			min-height: 26px;
		}

		QCheckBox:hover, QCheckBox:focus {
			color: rgb(238, 238, 238);
			background-color: rgba(9, 22, 30,0.5);
		}

		QCheckBox:disabled {
			color: rgb(87, 107, 126);
		}

		QCheckBox::indicator,
		QGroupBox::indicator {
			width: 18px;
			height: 18px;
		}

		QGroupBox::indicator {
			margin-left: 2px;
		}

		QCheckBox::indicator:unchecked {
			image: url(./Mintchip/Controls/square.svg);
		}

		QCheckBox::indicator:unchecked:hover,
		QGroupBox::indicator:unchecked:hover {
			border: none;
			image: url(./Mintchip/Controls/square-hover.svg);
		}

		QCheckBox::indicator:checked {
			image: url(./Mintchip/Controls/square-full.svg);
		}

		QCheckBox::indicator:checked:hover,
		QGroupBox::indicator:checked:hover {
			border: none;
			image: url(./Mintchip/Controls/square-full-hover.svg);
		}

		QCheckBox::indicator:checked:disabled,
		QGroupBox::indicator:checked:disabled {
			image: url(./Mintchip/Controls/square-full-disabled.svg);
		}

		QCheckBox::indicator:unchecked:disabled,
		QGroupBox::indicator:unchecked:disabled {
			image: url(./Mintchip/Controls/square-disabled.svg);
		}
	/* #endregion */

	/* #region Radio Buttons */
		QRadioButton {
			spacing: 5px;
			outline: none;
			color: rgb(238, 238, 238);
			margin-bottom: 2px;
			padding: 4px;
			border-radius: 6px;
			min-height: 26px;
		}

		QRadioButton:disabled {
			color: rgb(87, 107, 126);
		}

		QRadioButton::indicator {
			width: 18px;
			height: 18px;
		}

		QRadioButton::indicator:unchecked {
			image: url(./Mintchip/Controls/circle.svg);
		}


		QRadioButton::indicator:unchecked:hover,
		QRadioButton::indicator:unchecked:focus,
		QRadioButton::indicator:unchecked:pressed {
			border: none;
			outline: none;
			image: url(./Mintchip/Controls/circle-hover.svg);
		}

		QRadioButton::indicator:checked {
			border: none;
			outline: none;
			image: url(./Mintchip/Controls/circle-full.svg);
		}

		QRadioButton::indicator:checked:hover,
		QRadioButton::indicator:checked:focus,
		QRadioButton::indicator:checked:pressed {
			border: none;
			outline: none;
			image: url(./Mintchip/Controls/circle-full-hover.svg);
		}

		QRadioButton::indicator:checked:disabled {
			outline: none;
			image: url(./Mintchip/Controls/circle-full-disabled.svg);
		}

		QRadioButton::indicator:unchecked:disabled {
			image: url(./Mintchip/Controls/circle-disabled.svg);
		}
	/* #endregion */

	/* #region Progress Bars */
		QProgressBar {
			border: 2px solid rgb(47, 57, 66);
			border-radius: 4px;
			text-align: center;
		}

		QProgressBar::chunk {
			background-color: rgb(47, 194, 162);
		}
	/* #endregion */

	/* #region Combo Boxes (List Box) */
		QComboBox {
			background-color: rgb(9, 22, 30);
			border: 0px solid rgb(118, 121, 124);
			border-radius: 4px;
			padding: 8px;
			min-width: 64px;
			margin-left: 0px;
			margin-right: 8px;
		}

		QComboBox:on {
			padding-top: 8px;
			padding-left: 8px;
		}

		QComboBox QAbstractItemView {
			background-color: rgb(9, 22, 30); /* Dark Gray */
			border-radius: 4px;
			border: 0px transparent;
		}

		QComboBox::item {
			padding: 4px;
			max-height: 24px;
			margin: 0px;
			border-radius: 4px;
			border: none;
		}

		QComboBox::item:selected {
			background-color: rgb(47, 57, 66);
		}

		QComboBox::indicator {
			padding-left: 8px;
			width: 14px;
			height: 14px;
		}

		QComboBox::drop-down {
			subcontrol-origin: padding;
			subcontrol-position: top right;
			width: 30px;

			border-left-width: 0;
			border-left-color: rgb(87, 107, 126);
			border-left-style: solid;
			border-top-right-radius: 3px;
			border-bottom-right-radius: 3px;
		}


		QComboBox::down-arrow,
		QComboBox::down-arrow:pressed,
		QComboBox::down-arrow:focus {
			image: url(./Mintchip/Controls/angle-down-disabled.svg);
		}

		QComboBox::down-arrow:on,
		QComboBox::down-arrow:hover {
			image: url(./Mintchip/Controls/angle-down.svg);
			background-color: rgb(9, 22, 30); /* Dark Gray */
		}

		/* #region OBS Settings Pane Fixes */
		.OBSBasicSettings QComboBox QFrame {
			padding: 8px;
		}

		.OBSBasicSetting QComboBox * {
			margin: 20px;
		}
		/* #endregion */

	/* #endregion */

	/* #region Group Boxes */
		QGroupBox {
			border: 1px solid rgb(47, 57, 66);
			border-radius: 6px;
			padding-top: 16px;
			margin-top: 20px;
		}

		QGroupBox::title {
			color: rgb(47, 194, 162);
			left: 20px;
			padding-left: 5px;
			padding-right: 4px;
			padding-top: -14px;
			font-size: 24px !important;
		}
	/* #endregion */

	/* #region Splitters */

		QSplitter::handle {
			margin: 2px;
			width: 2px;
			height: 2px;
			border-radius: 2px;
			background-color: rgba(87, 107, 126, 0.1);
		}

		QSplitter::handle:hover {
			background-color: rgb(87, 107, 126);
		}
	/* #endregion */

	/* #region Sliders */
		QSlider::groove {
			border: none;
			border-radius: 2px;
		}

		QSlider::groove:horizontal {
			background-color: rgb(9, 22, 30);
			height: 4px;
		}

		QSlider::groove:vertical {
			background-color: rgb(33, 202, 161);
			width: 4px;
		}

		QSlider::handle {
			background-color: rgb(255, 255, 255);
			border-radius: 6px;
			height: 12px;
			width: 12px;
		}

		QSlider::handle:horizontal {
			margin: -4px 0;
		}

		QSlider::handle:vertical {
			margin: 0px -4px;
		}

		QSlider::handle:hover {
			background-color: rgb(163, 255, 231);
		}

		QSlider::handle:pressed {
			background-color: rgb(200, 200, 200);
		}

		QSlider::sub-page:horizontal:disabled,
		QSlider::groove:vertical:disabled {
			background-color: rgb(47, 57, 66);
			border-radius: 4px;
		}

		QSlider::sub-page {
			background-color: rgb(33, 202, 161);
			border-radius: 2px;
		}

		QSlider::sub-page:vertical {
			background-color: rgb(9, 22, 30);
		}

		QSlider::handle:disabled {
			background-color: rgb(87, 107, 126);
		}
	/* #endregion */

	/* #region NAME */

	/* #endregion */

/* #endregion */

/* #region OBS Elements */

	/* #region Theme Elements */
		* [themeID="addIconSmall"] {
		qproperty-icon: url(./Mintchip/plus.svg);
		}

		* [themeID="removeIconSmall"] {
		qproperty-icon: url(./Mintchip/minus.svg);
		}

		* [themeID="propertiesIconSmall"] {
		qproperty-icon: url(./Mintchip/Settings/cog.svg);
		}

		* [themeID="configIconSmall"] {
		qproperty-icon: url(./Mintchip/Settings/cog.svg);
		}

		* [themeID="refreshIconSmall"] {
		qproperty-icon: url(./Mintchip/refresh.svg);
		}

		* [themeID="upArrowIconSmall"] {
		qproperty-icon: url(./Mintchip/controls/angle-up.svg);
		}

		* [themeID="downArrowIconSmall"] {
		qproperty-icon: url(./Mintchip/controls/angle-down.svg);
		}

		* [themeID="pauseIconSmall"] {
		qproperty-icon: url(./Mintchip/pause.svg);
		}

		* [themeID="revertIcon"] {
		qproperty-icon: url(./Mintchip/undo.svg);
		}

		* [themeID="trashIconSmall"] {
		qproperty-icon: url(./Mintchip/delete.svg);
		}

		* [themeID="trashIcon"] {
		qproperty-icon: url(./Mintchip/delete.svg);
		}
	/* #endregion */

	/* #region Main Window */
		OBSBasic {
		qproperty-imageIcon: url(./Mintchip/Sources/image.svg);
		qproperty-colorIcon: url(./Mintchip/Sources/brush.svg);
		qproperty-slideshowIcon: url(./Mintchip/Sources/images.svg);
		qproperty-audioInputIcon: url(./Mintchip/Sources/microphone.svg);
		qproperty-audioOutputIcon: url(./Mintchip/Sources/sound.svg);
		qproperty-desktopCapIcon: url(./Mintchip/Sources/monitor.svg);
		qproperty-windowCapIcon: url(./Mintchip/Sources/window.svg);
		qproperty-gameCapIcon: url(./Mintchip/Sources/default.svg);
		qproperty-cameraIcon: url(./Mintchip/Sources/webcam.svg);
		qproperty-textIcon: url(./Mintchip/Sources/text.svg);
		qproperty-mediaIcon: url(./Mintchip/Settings/video.svg);
		qproperty-browserIcon: url(./Mintchip/Sources/globe.svg);
		qproperty-groupIcon: url(./Mintchip/Sources/folder.svg);
		qproperty-sceneIcon: url(./Mintchip/Sources/scene.svg);
		qproperty-defaultIcon: url(./Mintchip/Sources/default.svg);
		qproperty-windowIcon: url(./Mintchip/icon_256x256.png);

		qproperty-dockNestingEnabled: true;
		qproperty-unifiedTitleAndToolBarOnMac: false;
		}

		/* #region OBS Preview Window */
			#centralWidget {
				margin: 4px 4px 0px 4px;
			}

			OBSQTDisplay {
				qproperty-displayBackgroundColor: rgb(9, 22, 30);
			}

			#centralwidget > QFrame {
				border-radius: 6px;
				margin-top: 8px;
				padding: 8px;
			}

			#contextContainer,
			#contextContainer QFrame,
			#centralwidget > QFrame,
			#centralwidget > QFrame QFrame {
				background: rgb(9, 22, 30);
			}

			#contextContainer {
				margin-top: 8px;
				padding: 8px;
				border-radius: 6px;
			}

			#centralWidget QLabel {
				max-height: 24px;
			}

			OBSBasicPreview {
				margin: 16px;
			}
		/* #endregion */

		SourceTree,
		#stackedMixerArea,
		#controlsDockContents {
			padding: 8px;
			border-radius: 6px;
		}

		#stackedMixerArea,
		#controlsDockContents {
			margin: 8px;
		}
	/* #endregion */

	/* #region Mute Toggles */
		MuteCheckBox {
			outline: none;
			padding-right: -1px;
			margin: 0px 4px 0px 0px;
		}

		MuteCheckBox::indicator:checked {
			image: url(./Mintchip/Controls/mute.svg);
		}

		MuteCheckBox::indicator:unchecked {
			image: url(./Mintchip/Controls/audio.svg);
		}

		MuteCheckBox::indicator:unchecked:hover {
			image: url(./Mintchip/Controls/audio-hover.svg);
		}

		MuteCheckBox::indicator:unchecked:focus {
			image: url(./Mintchip/Controls/audio-hover.svg);
		}
		MuteCheckBox::indicator:checked:hover {
			image: url(./Mintchip/Controls/mute-hover.svg);
		}

		MuteCheckBox::indicator:checked:focus {
			image: url(./Mintchip/Controls/mute-hover.svg);
		}

		MuteCheckBox::indicator:checked:disabled {
			image: url(./Mintchip/Controls/mute.svg);
		}

		MuteCheckBox::indicator:unchecked:disabled {
			image: url(./Mintchip/Controls/audio.svg);
		}
	/* #endregion */

	/* #region Source Trees */
		SourceTree, SourceTree > * {
			background-color: rgb(9, 22, 30);
		}

		SourceTree::item {
			padding: 8px;
			min-height: 26px;
			border: none;
			border-radius: 6px;
			margin-bottom: 4px;
		}

		SourceTree::item * {
			min-height: 18px;
			min-width: 18px;
			padding: 0px;
			margin: 0px;
		}

		SourceTree::item QCheckBox {
			padding: 0px;
			margin: 0px;
		}

		SourceTree::item:selected:!active {
			color: rgb(239, 240, 241); /* White */
			background-color: rgb(14, 31, 39); /* Light Pink (Secondary Light) */
		}

		SourceTree::item:selected {
			background-color: rgb(14, 31, 39); /* Pink (Secondary) */
		}

		SourceTree::item:hover,
		SourceTree::item:disabled:hover,
		SourceTree::item:hover:!active {
			background-color: rgb(47, 57, 66); /* Cyan (Primary) */
			color: rgb(239, 240, 241); /* White */
		}

		SourceTree::Item:disabled {
			color: rgba(255,255,255,0.25);
		}

		SourceTree QLineEdit {
			padding-top: 0;
			padding-bottom: 0;
			padding-right: 0;
			padding-left: 2px;
			border: none;
			border-radius: none;
			margin-top: 4px;
			color: rgb(9, 22, 30);
		}

		SourceTreeSubItemCheckBox::indicator {
			border-radius: 6px;
			background-color: rgb(47, 57, 66);
			padding: -1px;
			width: 20px;
			height: 20px;
		}

		SourceTreeSubItemCheckBox::indicator:hover {
			background-color: rgb(87, 107, 126);
		}

		SourceTreeSubItemCheckBox::indicator:pressed {
			background-color: rgb(33, 202, 161);
		}

		SourceTreeSubItemCheckBox::indicator:checked,
		SourceTreeSubItemCheckBox::indicator:checked:hover {
			image: url(./Mintchip/Controls/angle-down.svg);
		}

		SourceTreeSubItemCheckBox::indicator:unchecked,
		SourceTreeSubItemCheckBox::indicator:unchecked:hover {
			image: url(./Mintchip/Controls/angle-up.svg);
		}

		OBSYoutubeActions {
			qproperty-thumbPlaceholder: url(:res/images/sources/image.svg);
		}
	/* #endregion */

	/* #region Folder/Visibility/Lock Icons */
		VisibilityCheckBox,
		LockedCheckBox,
		SourceTreeSubItemCheckBox {
		outline: none;
		background: transparent;
		}

		VisibilityCheckBox::indicator,
		SourceTreeSubItemCheckBox::indicator,
		LockedCheckBox::indicator {
		width: 18px;
		height: 18px;
		}

		OBSBasicFilters VisibilityCheckBox::indicator {
			margin-top: -18px;
		}

		VisibilityCheckBox::indicator:checked {
		image: url(./Mintchip/Controls/eye.svg);
		}

		VisibilityCheckBox::indicator:unchecked {
		image: url(./Mintchip/Controls/eye-slash.svg);
		}

		VisibilityCheckBox::indicator:checked:hover {
		image: url(./Mintchip/Controls/eye-hover.svg);
		}

		VisibilityCheckBox::indicator:unchecked:hover {
		image: url(./Mintchip/Controls/eye-slash-hover.svg);
		}

		LockedCheckBox::indicator:checked {
		image: url(./Mintchip/Controls/locked.svg);
		}

		LockedCheckBox::indicator:unchecked {
		image: url(./Mintchip/Controls/unlocked.svg);
		}

		LockedCheckBox::indicator:checked:hover {
		image: url(./Mintchip/Controls/locked-hover.svg);
		}

		LockedCheckBox::indicator:unchecked:hover {
		image: url(./Mintchip/Controls/unlocked-hover.svg);
		}
	/* #endregion */

	/* #region Transition Controls */

		#transitionsDock > * > * {
			margin-top: -8px;
		}

		#transitionProps,
		#transitions {
			margin-bottom: 8px;
		}

		#transitionProps {
			max-width: 16px;
			max-height: 16px;
			min-width: 16px;
			min-height: 16px;
			background-color: transparent;
			margin: 0px 0px 8px -8px;
			icon: url(./Mintchip/Settings/cog.svg);
		}

		#transitionProps:hover {
			icon: url(./Mintchip/Controls/cog-selected.svg);
		}

		#transitionProps:open,
		#transitionProps:focus {
			icon: url(./Mintchip/Controls/cog-hover.svg);
		}
	/* #endregion */

	/* #region Stream Control Buttons */
		#controlsDockContents QPushButton {
			margin: 2px 12px;
			background-color: rgb(47, 57, 66);
		}

		#controlsDockContents QPushButton:hover,
		#controlsDockContents QPushButton:checked {
			background-color: rgba(47, 194, 162, 180);
			border: 1px solid rgba(47, 194, 162, 180);
		}

		#controlsDockContents QPushButton:pressed {
			background-color: rgb(47, 194, 162);
			border: 1px solid rgb(47, 194, 162);
		}

		QPushButton#streamButton,
		QPushButton#recordButton {
			background-color: rgb(80, 50, 60);
			border: 1px solid rgb(80, 50, 60);
		}

		QPushButton:hover#streamButton,
		QPushButton:hover#recordButton {
			background-color: rgb(180, 60, 90);
			border: 1px solid rgb(180, 60, 90);
		}

		QPushButton:checked#streamButton,
		QPushButton:checked#recordButton {
			background-color: rgb(255, 0, 20);
			border: 1px solid rgb(255, 0, 20);
		}

		QPushButton:checked:hover#streamButton,
		QPushButton:checked:hover#recordButton {
			background-color: rgb(255, 80, 120);
			border: 1px solid rgb(255, 80, 120);
		}
	/* #endregion */

