	QLabel#scenesLabel,
	QLabel#sourcesLabel,
	QLabel#mixerLabel,
	QLabel#sceneTransitionsLabel {
		color: rgb(240, 240, 240); /* Pink (Secondary) */
		margin-top: 5px;
	}

	/* warning and error */
	QLabel#warningLabel {
		color: rgb(255, 148, 194); /* Light Pink (Secondary Light) */
		font-weight: bold;
	}

	QLabel#errorLabel {
		color: rgb(240, 240, 240); /* Dark Pink (Secondary Dark) */
		font-weight: bold;
	}
